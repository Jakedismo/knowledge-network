{
  "rules": {
    "no-unused-vars": "error",
    "no-undef": "error",
    "prefer-const": "error",
    "no-var": "error",
    "eqeqeq": "error",
    "no-duplicate-imports": "error",
    "no-console": "warn"
  },
  "env": {
    "browser": true,
    "node": true,
    "es2022": true
  },
  "ignore_patterns": [
    "node_modules/**",
    ".next/**",
    "out/**",
    "dist/**",
    "*.d.ts"
  ]
}